<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dbtest">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:name=".CountClass"
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:screenOrientation="landscape">
        </activity>
        <activity android:name=".KitchenActivity"
            android:screenOrientation="landscape"></activity>
        <activity android:name=".menuDBActivity"
            android:screenOrientation="landscape"></activity>
        <activity android:name=".MakesuccessActivity"
            android:theme="@style/Theme.MyDialog"
            android:screenOrientation="landscape"/>
        <activity android:name=".DayActivity"
            android:screenOrientation="landscape">
        </activity>
        <activity android:name=".NewMenuActivity"
            android:screenOrientation="landscape"/>
        <activity android:name=".StartActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SettingActivity"
            android:theme="@style/Theme.MyDialog"
            android:screenOrientation="landscape"/>
        <activity android:name=".RecipebookActivity"
            android:theme="@style/Theme.MyDialog"
            android:screenOrientation="landscape"/>
        <activity android:name=".WrongEndingActivity"
            android:screenOrientation="landscape"/>
        <activity android:name=".RecipebookActivity1"
            android:theme="@style/Theme.MyDialog"
            android:screenOrientation="landscape"/>
        <activity android:name=".RecipebookActivity2"
            android:theme="@style/Theme.MyDialog"
            android:screenOrientation="landscape"/>
       <activity android:name=".HelpActivity"
           android:screenOrientation="landscape"/>
        <activity android:name=".PrologueActivity"
            android:screenOrientation="landscape">
        </activity>
        <activity android:name=".TutoActivity"
            android:screenOrientation="landscape"/>
    </application>

</manifest>